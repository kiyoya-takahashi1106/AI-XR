using UnityEngine;

public class ItemControlSrcipt : MonoBehaviour
{
    // 相手から自分の中に通り抜けられたら呼ぶ
    private void OnTriggerEnter(Collider other)
    {
        if (other.gameObject.tag == "Player")
        {
            // アイテムを消す
            Destroy(gameObject);
        }
    }
}




"""
coinにこれのscriptを当てる
"""